#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int factorial(int n);
int main() {
    int n;
    scanf("%d",&n);
    printf("%d",factorial(n));
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
int factorial(int n){
    if(n<2)
        return 1;
    else
        return n*factorial(n-1);
}
